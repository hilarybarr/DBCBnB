					<h3><%=listing.description%> </h3><br>
					<% if listing.start_date %>
					 <p>Start Date: <%=listing.start_date%> </p><br> 
					<%end%>
					<% if listing.end_date %>
						 <p> End Date: <%=listing.end_date%></p>  
					<%end%>
					<p>Price: $<%=listing.price%> </p><br>
					<p>Location: <%=listing.location%> </p><br>
					<p>Max Occupants: <%=listing.max_avail%> </p><br> 
					<!-- <p>Room Sharing Available?: <%#=listing.roomsharing%> </p><br>  -->
